{
  "name": "@winches/auto-mock",
  "version": "0.1.2",
  "description": "约定式路由的mock工具",
  "main": "dist/app.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "auto-mock": "dist/src/bin.js"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "node src/bin.js",
    "dev": "node src/bin.js -w",
    "build": "tsup-node",
    "changlog": "npx conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "release": "bumpp --execute='npm run changlog' --all",
    "prepublish": "npm run build"
  },
  "keywords": [
    "mock",
    "auto-import",
    "Contractual routing",
    "mock 工具",
    "自动获取本地mock路径",
    "约定式mock路由"
  ],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winchesHe/auto-mock"
  },
  "dependencies": {
    "@winches/utils": "^0.0.3",
    "commander": "^10.0.0",
    "execa": "^5.0.0",
    "fastify": "^4.0.0",
    "nodemon": "^2.0.20",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "tap": "^16.1.0",
    "tsup": "^6.6.3"
  }
}
