{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "@antfu",
  "rules": {
    "n/prefer-global/process": "off",
    "curly": "off",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "error",
      {
        "args": "after-used",
        "argsIgnorePattern": "^_",
        "vars": "all",
        "varsIgnorePattern": "^_"
      }
    ],
    "no-console": "warn"
  }
}
